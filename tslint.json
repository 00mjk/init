{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
      "newline-after-var": [
        "error",
        "always"
      ],
      "no-unused-expressions": 0,
      "prefer-const": [
        "error",
        2
      ],
      "indent": [
        "error",
        2
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "always-multiline"
        }
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "multiline": false,
            "consistent": true
          },
          "ObjectPattern": {
            "multiline": false,
            "consistent": true
          }
        }
      ],
      "semi": [
        "error",
        "never"
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "time",
            "timeEnd",
            "table",
            "log"
          ]
        }
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "eqeqeq": [
        "error",
        "always"
      ],
      "spaced-comment": [
        "error",
        "always"
      ],
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "key-spacing": [
        "error",
        {
          "afterColon": true
        }
      ],
      "comma-spacing": [
        "error"
      ],
      "space-infix-ops": [
        "error",
        {
          "int32Hint": false
        }
      ],
      "computed-property-spacing": [
        "error",
        "never"
      ],
      "no-multi-spaces": [
        "error"
      ],
      "space-in-parens": [
        "error",
        "never"
      ]
    },
    "rules": {},
    "rulesDirectory": []
}